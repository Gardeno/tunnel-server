upstream wsgicluster {
	server unix:///tmp/tunnel.gardeno.sock;
}
server {
	listen 80;
        server_name tunnel.gardeno.global;
        error_log /var/log/tunnel.gardeno.global/error.log;
        access_log /var/log/tunnel.gardeno.global/access.log;
        location / {
                include uwsgi_params;
                uwsgi_pass wsgicluster;
        }
}